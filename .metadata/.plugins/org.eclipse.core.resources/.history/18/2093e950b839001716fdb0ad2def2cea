package br.com.SpringMVC;

@XmlRootElement(name = "Employee")
public class PessoaPOJO {
	 String name;
	 
	    String email;
	 
	    public String getName() {
	   	 return name;
	    }
	 
	    public void setName(String name) {
	   	 this.name = name;
	    }
	 
	    public String getEmail() {
	   	 return email;
	    }
	 
	    public void setEmail(String email) {
	   	 this.email = email;
	    }
	 
}
